package 圆的面积;
import java.util.*;

//一开始用的float s但是这样好像小数点后面的数就不准了，所以后面就都错了
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double Pi = Math.atan(1.0)*4;
		Scanner in = new Scanner(System.in);
		int r = in.nextInt();
		double s = Pi*r*r;
		System.out.printf("%.7f",s);
		in.close();
	}

}
